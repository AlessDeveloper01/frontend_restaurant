{
    "rewrites": [
        {
        "source": "/(.*)",
        "destination": "/"
        }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "no-cache"
            }
        ]
        }
    ],
    "trailingSlash": true,
    "cleanUrls": true
}
